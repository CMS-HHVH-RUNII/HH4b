#!/bin/bash
cd ${CMSSW_BASE}/src/HH4b/AABH/BiasStudies; eval `scramv1 run -sh`
root -b -q 'WSMakerForToys.C(0)'
root -b -q 'WSMakerForToys.C(1)'
combine datacards/toys_1600.txt -M GenerateOnly --setPhysicsModelParameters pdf_index=0 --freezeNuisances pdf_index,mjjlin_TT_,mjjlin_LL_ --toysFrequentist -t 100 --expectSignal 0 --saveToys -m 1600 
mv higgsCombineTest.GenerateOnly.mH1600.123456.root toys/higgsCombineTest.GenerateOnly.mH1600_mu0.root
combine datacards/toys_1600.txt -M MaxLikelihoodFit --toysFile toys/higgsCombineTest.GenerateOnly.mH1600_mu0.root -t 100 --rMin -30 --rMax 60 --minimizerTolerance 0.1 --setPhysicsModelParameters pdf_index=0 --freezeNuisances pdf_index,mjjlin_TT_,mjjlin_LL_
mv mlfit.root toys/mlfit_1600_mu0.root
root -b -q 'BiasStudies.C(1600, 0)'
